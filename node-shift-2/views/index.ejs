<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="style.css">
    <title>
        <%=title%>
    </title>
</head>

<body>
    <div class="blogs">
        <%blogs[2].forEach(blog =>{%>

            <div class="blog-card">
                <p>
                   Title: <%=blog.title%>
                </p>
                <p>
                   source: <%=blog.source%>
                </p>
                <p>
                    Published: <%=blog.publishedDate%>
                </p>
            </div>

            <%})%>
    </div>
</body>

</html>